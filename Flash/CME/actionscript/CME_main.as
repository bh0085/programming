package actionscript{	import flash.display.MovieClip	import flash.events.Event	import flash.display.Sprite	public class CME_main extends MovieClip{		public static var outlineColor= 0x999999		public static var internalOutlineColor = 0xBBBBBB		public static var buttonFillColor = 0xCCCCCC		public static var buttonGraphicColor = 0x888888		public static var textBGColor = 0xFFFFFF		public static var popupBGColor = 0xDDDDDD				public var mouseWin				public function CME_main():void{						var cme = new CME("testCME",this)			cme.addSlide(new CME_slide("This slide is a test of the class CME_slide. If this were a real slide it would have a lot more text.  You can pretend that this is a biased sentence and highlight it. Once Hightlighted Hit Enter to bring up a rating box. Rate the comment as positively or negatively biased!"))			cme.addSlide(new CME_slide("This slide is a test of the class CME. It is the second slide in the test CME. If this were a real CME there would be more than two slides"))			displayCME(cme)			mouseWin = new Sprite()			addChild(mouseWin)			addEventListener(Event.ENTER_FRAME,cNextFrame)					}		public function cNextFrame(e:Event):void{			mouseWin.x = mouseX			mouseWin.y = mouseY		}		public function displayCME(cme:CME):void{			cme.display(0)			addChild(cme)						cme.x = 25			cme.y = 13		}	}}