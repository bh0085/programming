package phystest{		import physics.PhysicsTest	import flash.events.MouseEvent	import flash.display.MovieClip		public class PlayButton extends MovieClip{				var thisPhysTest		public function PlayButton(physTest:PhysicsTest){			thisPhysTest = physTest			addEventListener(MouseEvent.MOUSE_DOWN, playDown)		}		public function playDown(event:MouseEvent){		thisPhysTest.unPauseThis()		thisPhysTest.removeChild(this)		thisPhysTest.addChild(thisPhysTest.thisPauseButton)	}	}	}