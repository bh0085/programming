package phystest{	import flash.events.Event	import flash.events.MouseEvent	import physics.PhysicsTest	import flash.display.MovieClip		public class Instructions extends MovieClip{				public var physicstest:PhysicsTest		public function Instructions(ptest:PhysicsTest){			physicstest = ptest			addEventListener(MouseEvent.MOUSE_DOWN,infoMouseDown)		}			function infoMouseDown(event:MouseEvent):void{		physicstest.removeInstructions()	}			}	}