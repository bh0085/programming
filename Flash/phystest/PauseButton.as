package phystest{		import physics.PhysicsTest	import flash.events.MouseEvent	import flash.display.MovieClip		public class PauseButton extends MovieClip {				var thisPhysTest		public function PauseButton(physTest:PhysicsTest){			thisPhysTest = physTest			addEventListener(MouseEvent.MOUSE_DOWN, pauseDown)		}		public function pauseDown(event:MouseEvent){		thisPhysTest.pauseThis()		thisPhysTest.removeChild(this)		thisPhysTest.addChild(thisPhysTest.thisPlayButton)	}	}}