package paths{	public class ObjectOutline extends Path2D{		public var lastSegUpdated:Number				public function updateThoseInNeed(num:Number):void{			for(var i = 1; i <= num ; i++){				var search = true				var nSearch = 0				while(search && nSearch < num){					seg = lastSegUpdated + 1					nSearch ++					if(subPathsInNeed[seg] == true){						search = false						subPathsInNeed[seg] = false					} else {					}					lastSegUpdated = seg				}			}		}			}}