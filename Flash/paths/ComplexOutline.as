package paths{	import flash.display.Sprite	public class ComplexOutline{				//an Array of outlines assumed to not intersect, assumed to belong to the same physical object		var outlines:Array		var holes		public function ComplexOutline():void{			outlines = new Array()		}		public function pushOutline(outline:CtdOutline):void{			outlines.push(outline)		}				public function drawFills():Sprite{			var fill = new Sprite()			for(var i = 0 ; i < outlines.length ; i++){				var f = outlines[i].drawFill()				var fillContainer = new Sprite()				fillContainer.addChild(f)				fillContainer.alpha =.5				fill.addChild(fillContainer)			}			return fill		}		public function drawOutlines():Sprite{			var outline = new Sprite()			for(var i = 0; i<outlines.length ; i++){				outline.addChild(outlines[i].drawOutline(false,null,5,0x000000,.5))			}			return outline		}	}}