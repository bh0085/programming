package physics.pscript{	import tweens.easeInEaseOut	public class oShowScript extends OverlayScript{				override public function advance(...args):void{			var maxFrame = 10			thisOverlay.visible = true			thisOverlay.x = easeInEaseOut.position([1,maxFrame],[-200,0],frame)			thisOverlay.setOpacity(1 * frame/maxFrame)			if (frame >= maxFrame) {				scriptDone = true}					}			}}