package physics.pscript{	import physics.objects.oscript.ObjectScript	import physics.objects.PhysObject	import flash.display.Sprite	public class stickToObject extends ObjectScript{		public var stuckTo		public var whichStuck					override public function advance(...args):void{			var newOutline = stuckTo.thisOutline.intersectWith(whichStuck.thisOutline.outline,2)						if(newOutline != null){			stuckTo.thisOutline.outline = newOutline			stuckTo.thisOutline.cleanOutline()			stuckTo.thisOutline.setObject(stuckTo)			newOutline.genSegments()			newOutline.genOrientation(0xFF0000)			newOutline.genFill(0xFFFFFF)			stuckTo.setDefaults()									var gfx = stuckTo.thisOutline.drawShape(0xFFFFFF,30)			stuckTo.setGfxMask(gfx)			var gAdd = whichStuck.getGraphic()			gAdd.rotation = whichStuck.rotation - stuckTo.rotation			gAdd.x = whichStuck.getX() - stuckTo.getX()			gAdd.y = whichStuck.getY() - stuckTo.getY()			gAdd.mask = null			stuckTo.addGraphic(gAdd)									whichStuck.destroyDefault()			} else {trace("nullOutline")}			scriptDone = true		}				public function setWhichStuckTo(s:PhysObject):void{			stuckTo = s		}		public function setWhichIsStuck(s:PhysObject):void{			whichStuck = s		}					}}