package physics.pscript {	import physics.PhysObject		public class ObjectScript extends PhysScript{		public var thisObject:PhysObject		public var objectGraphics:Array				public function setObject(t:PhysObject){			thisObject = t		}	}			override public function endScript():void{		if objectGraphics != null){			for (var i = 0; i < objectGraphics.length ; i++)				thisObject.removeChild(objectGraphics[i])				objectGraphics[i] = null			}		}	}}