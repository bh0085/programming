package physics.pscript {	import physics.PhysUniverse	import flash.display.DisplayObject	import flash.utils.getQualifiedClassName		public class UniverseScript extends PhysScript{		public var thisUniverse:PhysUniverse		public var universeGraphics:Array				public function setUniverse(u:PhysUniverse){			thisUniverse = u		}					override public function endScriptDefault():void{		if (universeGraphics != null){			for (var i = 0; i < universeGraphics.length ; i++)				thisUniverse.removeChild(universeGraphics[i])				universeGraphics[i] = null							}			endScript()		}		public function addGraphic(graphic : DisplayObject):void{			if (universeGraphics == null){universeGraphics = new Array}			universeGraphics.push(graphic)			thisUniverse.addChild(graphic)		}		public function howManyOfThisScript():Number{			var num = 0			for ( var i = 0 ; i< thisUniverse.scripts.length ; i ++)				{					var className = flash.utils.getQualifiedClassName(thisUniverse.scripts[i]);					if (className == flash.utils.getQualifiedClassName(this)){num++ }				}			return num		}	}}