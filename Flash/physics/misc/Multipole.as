package physics.misc{	public class Multipole{		public var masses = new Array()		public var charges = new Array()		public var customCharges = new Array()		public var ptsXY = new Array()				public function pushPole( mass:Number, charge:Number, customCharge:Array,xy:Array){			masses.push(mass)			charges.push(charge)			customCharges.push(customCharge)			ptsXY.push(xy)		}	}}