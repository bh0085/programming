package physics {		import flash.display.Shape		public class PhysWallSegment extends Shape {		var seg0:Array		var seg1:Array		var color		public function PhysWallSegment(s0:Array, s1:Array, ... args) {						if (args.length >= 1){color = args[0]} else {color = 0x000000}						seg0 = s0			seg1 = s1			graphics.lineStyle(100,color)			graphics.moveTo(seg0[0], seg0[1])		    graphics.lineTo(seg1[0], seg1[1])													}			}	}