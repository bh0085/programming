package physics.projectile{		import physics.pscript.ProjScript	import tanks.tank.Explosion	public class NewtSScript extends ProjScript{		override public function advance(...args):void{			// thisProjectile.setCharge(-1 * Math.pow(10,-1) * frame)			if (frame == 1){thisProjectile.setXY([thisProjectile.initialXY[0],thisProjectile.initialXY[1]])}			if (frame == 1){				var impulsemag = (thisProjectile.initialPower * thisProjectile.getMass() * (6 - frame) * 100)				thisProjectile.exertImpulseOn([Math.cos(thisProjectile.initialAngle)*impulsemag,Math.sin(thisProjectile.initialAngle)*impulsemag])			}			if (frame == 50) {scriptDone = true}		}				override public function endScript():void{			var exp = new Explosion()			exp.setTank(thisProjectile.thisTank)			exp.setInitialXY([thisProjectile.getX(),thisProjectile.getY()])			exp.setAngle(thisProjectile.angle)			exp.setPower(10 )			exp.colType = thisProjectile.colType		}			}	}