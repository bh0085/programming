package actionscript{	public class Fares{		static var maxFare:Number		public static function getFare(p1:GlooPlace, p2:GlooPlace):Number{			var lL1 = p1.getLatLon()			var lL2 = p2.getLatLon()			var vec = [lL2[0] - lL1[0],lL2[1] - lL1[1]]			var dist =Math.sqrt(Math.pow(vec[0],2) + Math.pow(vec[1],2))			var fare = dist * (10 + Math.random())			return fare		}		public static function resetMax():void{			maxFare = -1		}		public static function getMinFare(p1:GlooPlace , p2:GlooPlace):Array{			var ps1 = p1.getAirports()			var ps2 = p2.getAirports()			var minI = -1			var minJ = -1			var minFare = 0			for(var i = 0 ; i < ps1.length ; i++){				for(var j = 0 ; j < ps2.length ; j++){					var fare = getFare(ps1[i],ps2[j])					ps2[j].setFare(fare)					if(minI == -1 || fare < minFare){						minI = i						minJ = j						minFare = fare					}					if(fare > maxFare){						maxFare = fare					}				}			}			return ([minFare,[minI,minJ]])		}	}}