package math{	public dynamic class MathArray extends Array{		public function MathArray( ... args){			if (args.length == 1) {super(args[0])}			if (args.length != 1) {				for (var i:Number = 0 ; i < args.length ; i++){					this.push(args[i])				}			}						}				public function total():Number {			var sum:Number = 0			for (var i:Number = 0 ; i < this.length ; i++){				sum += this[i]}			return sum		}									public function mean():Number {			return this.total() / this.length		}	}}