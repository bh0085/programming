package tanks.projectiles{	import flash.display.Shape;	import paths.PhysOutline;	import tanks.scripts.BombScript	import tanks.explosions.Explosion	import flash.events.Event	import math.VectorMath2D	public class StickyDud extends Projectile{		public function StickyDud():void {			objectCanStick = true			setMass(1)			setCharge(0)			thisOutline = PhysOutline.sphereOutline(50,10);			thisOutline.setObject(this);			setGraphic(thisOutline.drawShape(0x555555,10));		}		override public function createScript():void {			thisScript = new BombScript();			thisScript.setExplosion(thisExplosion)			thisScript.setProjectile(this);			thisScript.setTank(thisTank);			thisScript.beginScript();		}		override public function pNextFrame(e:Event):void{			if(isDestroyed){thisScript.scriptDone = true}			if(thisScript.advanceDefault(0)){thisScript.endScriptDefault()}else{}			 			rotateTo(VectorMath2D.vectorGetAngle(getVel()))		}		override public function defaultExplosion():Explosion{			return null		}	}}				