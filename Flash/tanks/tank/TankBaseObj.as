package tanks.tank{	import flash.events.MouseEvent	import flash.utils.getTimer	import flash.events.Event		import physics.PhysUniverse	import physics.objects.PhysObject		import paths.PhysOutline	import paths.CtdOutline	import paths.Node			public class TankBaseObj extends PhysObject{		public function TankBaseObj(){		setMass(1000)				thisOutline = new PhysOutline()		var o = new CtdOutline()		o.object = this		var node2 = new Node(50,-3)		o.pushNode(node2)				var node1 = new Node(-60,-4)		o.pushNode(node1)		var node3 = new Node(-10,-60)		node3.isSticky = false		o.pushNode(node3)				var node4 = new Node(10,-60)		node4.isSticky = false		o.pushNode(node4)		thisOutline.setOutline(o)		thisOutline.outline.genFill(0xFFFFFF)		thisOutline.outline.genOrientation()		thisOutline.outline.genSegments()		setGraphic(thisOutline.drawShape(3,0x333333,Math.random()*Math.pow(256,3),false))				thisOutline.setObject(this)		}					}	}