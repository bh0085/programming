package tanks.tank{		import flash.events.Event	import physics.PhysObject	import flash.display.MovieClip		public class Missile extends PhysObject{				var tankPar:Tank		public function Missile(tankParent:Tank){			tankPar = tankParent			setGraphic(new MissileGraphic)			setCharge(Math.pow(10,-2))			this.addEventListener(Event.ENTER_FRAME,missileEnterFrame,false, -10)		}		public function missileEnterFrame(event:Event):void{			var vel = Math.sqrt(Math.pow(xvel,2) + Math.pow(yvel,2))			var motionDegrees = (Math.asin(yvel/vel)) * 180 / Math.PI			if (xvel/vel <0) {motionDegrees = 180 - motionDegrees}			this.rotation = motionDegrees  			if (numConsecutiveWallHits > 0){				removeEventListener(Event.ENTER_FRAME,missileEnterFrame)				tankPar.triggerExplosion([x,y],rotation)				trace(getUniverseIndex())				thisUniverse.removeObj(getUniverseIndex())			}					}					}}