package tanks.tank.tankscripts {		public class TankAIScript extends TankScript {				override public function advance(...args):void{			if(!thisTank.isSleeping){			if (((frame % 20 )  == 0)&& Math.random() > .5){ 	thisTank.addTankScript(new tForward())}  			if (((frame % 20) == 10)&& Math.random() > .5){	thisTank.addTankScript(new tBackward())}  			if ((frame % 30 == 0) && Math.random() >.75){thisTank.addTankScript(new tTurretUp())}			if ((frame % 30 == 15) && Math.random() >.75){thisTank.addTankScript(new tTurretDown())}			if ((frame % 50 == 15) && Math.random() >.75){thisTank.addTankScript(new tFlipForward())}			}							}	}	}