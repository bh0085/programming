package tanks.tank.tankscripts {		import tanks.tank.Tank		public class tFlipOver extends TankScript{				var startingAngle:Number		var targetAngle:Number		override public function beginScript():void{			startingAngle = thisTank.angle			targetAngle = startingAngle + Math.PI		}		override public function advance():void{								var deltaRad = targetAngle - thisTank.angle									var k = (20  * deltaRad) /  5			thisTank.torqueThrust(k)						if ( Math.abs(deltaRad) < .5 ){thisTank.dampen([0,0,.1])}			if (frame  > 10) {scriptDone = true}		}	}}