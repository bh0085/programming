package tanks.tank.tankscripts {		import flash.display.Shape	import flash.events.KeyboardEvent	import flash.ui.Keyboard		import tanks.tank.Tank		public class tTurretUp extends TankScript{		override public function advance(...args):void{			var dRot = - 6 *.5 //* ( Math.abs(frame - 5)) / 5			thisTank.rotateTurret( - dRot )			var rotateCof = .01			thisTank.exertTorqueImpulseOn( [0,0,+10 * dRot * thisTank.getMomentOfInertia() * rotateCof] )			if (frame  > 10) { scriptDone = true}		}	}}