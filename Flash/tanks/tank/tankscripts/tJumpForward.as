package tanks.tank.tankscripts {		import tanks.tank.Tank		public class tJumpForward extends TankScript{		public var initialAngle:Number		override public function advance():void{			if (frame ==1){initialAngle = thisTank.angle}						var angleDelta = initialAngle - thisTank.angle			var search = true			 while( search ){				if(angleDelta < -1 * Math.PI) { angleDelta += Math.PI * 2 }				if(angleDelta > Math.PI) {angleDelta -= Math.PI*2 }				if(Math.abs(angleDelta) < Math.PI) {search = false}			}									var k = (10 +frame) / 10			thisTank.torqueThrust(k)						if (frame > 5){				var k = ( 20* angleDelta) / 10				thisTank.dampen([1,1,.5])				thisTank.torqueThrust(k)			}						k = (15 - frame) * 30						thisTank.thrustUpwards(k)			k = .2			thisTank.thrustForward(k)						if (frame  > 10) {scriptDone = true}		}	}}