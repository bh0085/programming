package tanks.tank.tankscripts {		import tanks.tank.Tank	import flash.display.Shape		public class tGoHome extends TankScript{		override public function advance():void{						if(frame == 1) {				var shape = new Shape()				shape.graphics.beginFill(0x0000BB)				var dist = + 25 + 50 * howManyOfThisScript()				var angle = .5				var dx = Math.cos(angle - Math.PI / 2) * dist				var dy = Math.sin(angle - Math.PI / 2) * dist				shape.graphics.drawCircle(dx,dy,15)				shape.alpha = .3				addGraphic(shape)			}			if(frame ==1) thisTank.addTankScript(new tPointTo([0,0]))			thisTank.dampen([.9,.9,.25])			thisTank.thrustForward(.5)			//thisTank.thrustForward(10)							if (frame  > 10) {scriptDone = true}		}	}}