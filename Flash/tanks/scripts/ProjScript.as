package tanks.scripts {	import tanks.tank.Tank	import physics.pscript.PhysScript	import flash.display.DisplayObject	import flash.utils.getQualifiedClassName	import flash.events.Event	import tanks.projectiles.Projectile	import tanks.explosions.Explosion		public class ProjScript extends PhysScript{		public var thisProjectile:Projectile		public var originalTank:Tank		public var projectileGraphics:Array		public var thisExplosion:Explosion				public function setProjectile(p:Projectile){thisProjectile = p}				public function setTank(t:Tank){originalTank = t}			public function ProjScript():void{}			override public function endScriptDefault():void{		if (projectileGraphics != null){			for (var i = 0; i < projectileGraphics.length ; i++)				thisProjectile.removeChild(projectileGraphics[i])				projectileGraphics[i] = null							}			endScript()			thisProjectile.destroyDefault()		}		public function addGraphic(graphic : DisplayObject):void{			if (projectileGraphics == null){projectileGraphics = new Array}			projectileGraphics.push(graphic)			thisProjectile.addChild(graphic)		}		public function setExplosion(expl:Explosion):void{			thisExplosion  = expl		}		override public function endScript():void{			var expl = thisProjectile.thisExplosion			var tank = thisProjectile.thisTank			var initXY = [thisProjectile.getX(),thisProjectile.getY()]			var angle = thisProjectile.angle			var power = thisProjectile.thisExplPower			if(expl != null){Explosion.initializeExplosion(expl,initXY,tank,power,angle)}		}	}}