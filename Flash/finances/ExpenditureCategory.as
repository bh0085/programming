package finances{	import flash.display.Sprite		public class ExpenditureCategory{		public var expTotal:Number		public var name:String		public var expenditures:Array = new Array()		public var expGraphic:Sprite = new Sprite()					public function ExpenditureCategory(n){name = n}			public function pushExpenditure(e:Expenditure){				expenditures.push(e)				expTotal += e.cost			}	}	}